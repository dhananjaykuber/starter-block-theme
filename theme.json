{
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json",
	"settings": {
		"appearanceTools": true,
		"spacing": {
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Quicksand",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": ["file:./assets/fonts/Quicksand-Light.ttf"]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": ["file:./assets/fonts/Quicksand-Regular.ttf"]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": ["file:./assets/fonts/Quicksand-Medium.ttf"]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": ["file:./assets/fonts/Quicksand-SemiBold.ttf"]
						},
						{
							"fontFamily": "Quicksand",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": ["file:./assets/fonts/Quicksand-Bold.ttf"]
						}
					],
					"fontFamily": "\"Quicksand\", serif",
					"name": "Quicksand",
					"slug": "body"
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, avenir next, avenir, segoe ui, helvetica neue, helvetica, Cantarell, Ubuntu, roboto, noto, arial, sans-serif",
					"name": "System Sans-serif",
					"slug": "system-sans-serif"
				},
				{
					"fontFamily": "Iowan Old Style, Apple Garamond, Baskerville, Times New Roman, Droid Serif, Times, Source Serif Pro, serif, Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol",
					"name": "System Serif",
					"slug": "system-Serif"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					},
					"name": "ExtraSmall",
					"size": "1.25rem",
					"slug": "x-small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.5rem"
					},
					"name": "Small",
					"size": "1.5rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": "1rem",
						"max": "1.25rem"
					},
					"name": "Regular",
					"size": "1.25rem",
					"slug": "regular"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "1.25rem"
					},
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.75rem",
						"max": "4rem"
					},
					"name": "Large",
					"size": "4rem",
					"slug": "large"
				},
				{
					"fluid": {
						"min": "2rem",
						"max": "4.5rem"
					},
					"name": "Extra Large",
					"size": "4.5rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"min": "3rem",
						"max": "5.5rem"
					},
					"name": "Extra Extra Large",
					"size": "5.5rem",
					"slug": "xx-large"
				}
			],
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3
}
